/*chat directive fade in animation Start*/
/*.visible-out{

    opacity: 0;
    animation: fade-out 1s;

}
.visible-out:hover{

    background-color: red;
}*/
.visible-in{
  opacity: 1;
  animation: fadein 1s;
}
@keyframes fade-out {
    0% { display:block; opacity: 1; }
    100% { display:none; opacity: 0; }
}


@keyframes fadein {
    from {
        opacity:0;
    }
    to {
        opacity:1;
    }
}
@-moz-keyframes fadein { /* Firefox */
    from {
        opacity:0;
    }
    to {
        opacity:1;
    }
}
@-webkit-keyframes fadein { /* Safari and Chrome */
    from {
        opacity:0;
    }
    to {
        opacity:1;
    }
}
@-o-keyframes fadein { /* Opera */
    from {
        opacity:0;
    }
    to {
        opacity: 1;
    }
}
/*chat directive fade in animation End*/




.chat-block.fadein {
  opacity: 1;   
}
.chat-block.ng-hide-add, .chat-block.ng-hide-remove {
  transition: all linear 0.5s;
}

.chat-block.ng-hide {
  line-height: 0;
  opacity: 0;
  padding: 0 10px;
}
/*End of angular animations*/

